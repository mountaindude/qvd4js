{
  "name": "qvd4js",
  "version": "1.0.5",
  "type": "module",
  "description": "Utility library for reading/writing Qlik View Data (QVD) files in JavaScript.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest . --coverage",
    "clean": "rimraf -rf dist",
    "bench": "node benchmarks/qvd-parsing.bench.js",
    "bench:ci": "node benchmarks/qvd-parsing.bench.js --ci"
  },
  "author": {
    "name": "Constantin Müller",
    "email": "info@mueller-constantin.de",
    "url": "https://mueller-constantin.de"
  },
  "contributors": [
    {
      "name": "Göran Sander",
      "email": "goran@ptarmiganlabs.com",
      "url": "https://github.com/mountaindude"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MuellerConstantin/qvd4js.git"
  },
  "keywords": [
    "qvd",
    "qlik",
    "qlik sense",
    "qlikview",
    "esm",
    "commonjs",
    "dual-package",
    "data-analysis"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.38.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tinybench": "^5.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "peerDependencies": {
    "xml2js": "^0.6.2"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
